<!--pages/credential/credential.wxml-->
<view class="main">
	<view class="main1 component">
		<view class="userinfo">
			<image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
			<text class="userinfo-nickname">{{userInfo.nickName}}</text>
		</view>
  	<view class="credential">
    	<image src="{{credentialUrl}}" class="credential-img"></image>
  	</view>
	</view>
</view>